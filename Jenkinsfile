pipeline {
    
    agent any

    stages{

        stage("Checkout SCM"){
            steps{
                echo "Checkout SCM"
            }
        }

        stage("Build"){
            steps{
                echo "Build"
            }
        }

        stage("Push to Dockerhub"){
            steps{
                echo "Push to Dockerhub"
            }
        }

        stage("Deploy"){
            steps{
                echo "Deploy"
            }
        }
    }
}
